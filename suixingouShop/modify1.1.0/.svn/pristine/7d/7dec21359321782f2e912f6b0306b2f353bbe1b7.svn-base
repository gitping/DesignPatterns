package com.yto.zhang.util.modle;



/**
 * 商家获取我的快递单
 * 
 * @author jingzhong
 * 
 */

public class ShopExpressOrderReqJo extends UUID {

	public static String WAITING = "0";
	public static String RECEIVING = "1";
	public static String FINISHED = "2";
	/**
	 * 店铺id
	 */
	private Integer id;

	/**
	 * 类型 WAITING(0, "待接单") ,NOHAVE_EXPRESS(1,"待取件"),FINISHED(2,"已经完成")
	 */

	private Integer type;

	/**
	 * 页码
	 */
	private Integer pageIndex;

	public Integer getId() {
		return id;
	}

	public void setId(Integer id) {
		this.id = id;
	}

	public Integer getType() {
		return type;
	}

	public void setType(Integer type) {
		this.type = type;
	}

	public Integer getPageIndex() {
		return pageIndex;
	}

	public void setPageIndex(Integer pageIndex) {
		this.pageIndex = pageIndex;
	}

}
