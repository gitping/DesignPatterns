                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/store_guideset_tw" />
                </TableRow>

                <TextView
                    android:layout_width="fill_parent"
                    android:layout_height="1dp"
                    android:background="@color/fcolor" />

                <TableRow
                    android:id="@+id/tableRow4"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" >

                    <Button
                        android:id="@+id/producti_menu_butyj"
                        style="@style/Store_myMenuButStyle"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/store_guideset_yj" />
                </TableRow>

                <TextView
                    android:layout_width="fill_parent"
                    android:layout_height="1dp"
                    android:background="@color/fcolor" />

                <TableRow
                    android:id="@+id/tableRow5"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" >

                    <Button
                        android:id="@+id/producti_menu_butryp"
                        style="@style/Store_myMenuButStyle"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/store_guideset_ryp" />
                </TableRow>

                <TextView
                    android:layout_width="fill_parent"
                    android:layout_height="1dp"
                    android:background="@color/fcolor" />

                <TableRow
                    android:id="@+id/tableRow6"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" >

                    <Button
                        android:id="@+id/producti_menu_butsx"
                        style="@style/Store_myMenuButStyle"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/store_guideset_sx" />
                </TableRow>

                <TextView
                    android:layout_width="fill_parent"
                    android:layout_height="1dp"
                    android:background="@color/fcolor" />

                <TableRow
                    android:id="@+id/tableRow7"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" >

                    <Button
                        android:id="@+id/producti_menu_butqt"
                        style="@style/Store_myMenuButStyle"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/store_guideset_qt" />
                </TableRow>

                <TextView
                    android:layout_width="fill_parent"
                    android:layout_height="1dp"
                    android:background="@color/fcolor" />
                 <TableRow
                    android:id="@+id/tableRow8"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" >
                
               <RelativeLayout
                        android:layout_width="fill_parent"
                        android:layout_height="match_parent"
                        >
                    <RelativeLayout 
                        android:id="@+id/addrela"
                        android:clickable="true"
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:layout_alignParentBottom="true"
                        android:layout_centerInParent="true"
                        android:background="@color/addnewpro"
                        >
                    <ImageButton
                        android:id="@+id/addnewp"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:src="@drawable/addnewproicon" 
                        android:layout_alignParentLeft="true"
                        android:layout_centerInParent="true"
                        android:background="@null"
                        />
                    <TextView 
                        android:id="@+id/texta"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="添加"
                        android:textSize="14sp"
                        android:textColor="@color/fcolor"
                        android:layout_toRightOf="@id/addnewp"
                        android:layout_marginLeft="10dip"
                        />
                    <TextView 
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="新商品  "
                        android:textSize="14sp"
                        android:textColor="@color/fcolor"
                        android:layout_below="@id/texta"
                        android:layout_toRightOf="@id/addnewp"
                        android:layout_marginLeft="10dip"
                        />
                    </RelativeLayout>
                    </RelativeLayout>
                    </TableRow>
                

                <!--
                    <TableRow
                    android:id="@+id/tableRow8"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" >

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content" >

                        <Button
                            android:id="@+id/producti_menu_butyx"
                            style="@style/Store_myMenuButStyle"
