            <RelativeLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" >

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="horizontal" >

                    <LinearLayout
                        android:layout_width="1dip"
                        android:layout_height="match_parent"
                        android:layout_weight="0.1"
                        android:background="@color/huitop" />

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:layout_weight="20"
                        android:background="#fff"
                        android:orientation="vertical" >

                        <TextView
                            android:id="@+id/iprotext"
                            android:layout_width="fill_parent"
                            android:layout_height="wrap_content"
                            android:background="@color/huitop"
                            android:gravity="center_vertical"
                            android:paddingBottom="3dip"
                            android:paddingLeft="10dip"
                            android:paddingTop="3dip"
                            android:textColor="@color/white"
                            android:textSize="16sp" />

                        <com.yto.suixingoustore.util.view.listview.XListView
                            android:id="@+id/iproduct_listview"
                            android:layout_width="fill_parent"
                            android:layout_height="match_parent"
                            android:layout_weight="9.0"
                            android:background="@color/white"
                            android:cacheColorHint="#00000000"
                            android:divider="@color/black"
                            android:dividerHeight="0.6dp"
                            android:listSelector="#00000000"
                            android:scrollbars="none"
                            android:visibility="visible" >
                        </com.yto.suixingoustore.util.view.listview.XListView>

                        <!--
                           <Button
                            android:id="@+id/pricesummit"
                            android:layout_width="fill_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="5dip"
                            android:layout_marginLeft="3dip"
                            android:layout_marginRight="2dip"
                            android:layout_marginTop="5dip"
                            android:layout_weight="0.1"
                            android:background="@drawable/green_button_selector"
                            android:gravity="center"
                            android:paddingBottom="5dip"
                            android:paddingTop="5dip"
                            android:text="@string/isummit"
                            android:textColor="@color/white"
                            android:textSize="15sp"
                            android:visibility="gone" />
                        -->
                    </LinearLayout>

                    <com.frame.view.slidealphaposion.QuickAlphabeticBar
                        android:id="@+id/fast_scroller"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:layout_gravity="top|right|center"
                        android:layout_weight="0.1"
                        android:background="@drawable/dic_background"
                        android:scaleType="centerInside" />
                </LinearLayout>

                <TextView
                    android:id="@+id/fast_position"
                    android:layout_width="70dip"
                    android:layout_height="70dip"
                    android:layout_centerInParent="true"
                    android:layout_gravity="center_horizontal|top"
                    android:layout_margin="34dip"
                    android:background="@drawable/sort_icon_bg_click"
                    android:gravity="center"
                    android:padding="2dip"
                    android:textColor="#404040"
                    android:textSize="40dip"
                    android:visibility="invisible" />
            </RelativeLayout>
        </LinearLayout>

