<resources xmlns:android="http://schemas.android.com/apk/res/android">

    <!-- frame -->
    <style name="fcommon_circular_dark_yellow_yellow_style">
        <item name="android:textColor">@color/back</item>
        <item name="android:textSize">16sp</item>
        <item name="android:background">@drawable/fcommon_circular_yellow_btn_selector</item>
    </style>

    <style name="ftext_color_style">
        <item name="android:textColor">@drawable/ftext_click_seletor</item>
        <item name="android:clickable">true</item>
    </style>

    <style name="FLeftMeunPoup">
        <item name="android:windowEnterAnimation">@anim/fleftmenu_open</item>
        <item name="android:windowExitAnimation">@anim/fleftmenu_close</item>
    </style>
    
    <!-- iphone -->
     <!-- Dialog的风格布局 -->
    <style name="StyleBarTitle">
        <item name="android:layout_gravity">center_vertical</item>
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_weight">1</item>
        <item name="android:gravity">center_horizontal</item>
        <item name="android:textColor">@color/white_color</item>
        <item name="android:shadowColor">@color/black</item>
        <item name="android:shadowDx">1</item>
        <item name="android:shadowDy">1</item>
        <item name="android:shadowRadius">2</item>
    </style>
    
    <!-- 菜单button -->
    <style name="fleftMeunbntStyle">
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">20sp</item>
        <item name="android:background">@android:color/transparent</item>
        <item name="android:drawablePadding">15dip</item>
        <item name="android:gravity">center</item>
        <item name="android:padding">5dip</item>
    </style>
    
    <!-- store -->
    <!-- myshop menu -->
      <style name="Store_myMenuButStyle">
        <item name="android:textColor">@color/black</item>
        <item name="android:textSize">20sp</item>
        <item name="android:background">@android:color/transparent</item>
        <item name="android:drawablePadding">15dip</item>
        <item name="android:gravity">center</item>
        <item name="android:padding">5dip</item>
    </style>
    
      <style name="frameDialogStyle">
        <item name="android:layout_gravity">center_vertical</item>
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_weight">1</item>
        <item name="android:gravity">center_horizontal</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:shadowColor">@color/black</item>
        <item name="android:shadowDx">1</item>
        <item name="android:shadowDy">1</item>
        <item name="android:shadowRadius">2</item>
    </style>
    
    

</resources>