<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android"
          xmlns:app="http://schemas.android.com/apk/res-auto">
    <item android:state_selected="true" android:drawable="@drawable/icon_date_unselect"/>
    <item android:state_pressed="true" android:drawable="@drawable/icon_date_unselect"/>
    <item app:state_current_month="false">
        <shape android:shape="rectangle">
            <solid android:color="@color/white"/>
        </shape>
    </item>
    <item app:state_today="true" android:drawable="@color/maintitle_gray616161"/>
    <item>
        <shape android:shape="rectangle">
            <solid android:color="@color/white"/>
        </shape>
    </item>
</selector>
